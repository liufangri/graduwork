<?xml version="1.0" encoding="UTF-8"?>
<beans xmlns="http://www.springframework.org/schema/beans"
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xmlns:Context="http://www.springframework.org/schema/context"
	xmlns:aop="http://www.springframework.org/schema/aop"
	xmlns:tx="http://www.springframework.org/schema/tx"
	xsi:schemaLocation="http://www.springframework.org/schema/beans http://www.springframework.org/schema/beans/spring-beans.xsd
	http://www.springframework.org/schema/context http://www.springframework.org/schema/context/spring-context.xsd"
	default-autowire="byName" >
	<bean id="testService" class="com.sxy.graduwork.service.TestService" scope="singleton"></bean>
	<bean id="indexService" class="com.sxy.graduwork.service.IndexService" scope="singleton"></bean>	
	<bean id="acmService" class="com.sxy.graduwork.service.ACMService" scope="singleton"></bean>
	<bean id="searchConfigService" class="com.sxy.graduwork.service.SearchConfigService" scope="singleton"></bean>
	<bean id="searchService" class="com.sxy.graduwork.service.SearchService" scope="singleton"></bean>
</beans>